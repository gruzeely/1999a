fun main() {
   // Obs. listOf() -> IMUTAVEL 
   // 
   // mutablelistOf() --> armazena qualquer tipo de valores conjuntos 
   // mutablelistOf<Tipo de valor> -> armazena valores especificos de seu tipo 
   /*
   var comida = mutableListOf<String>("Strogonoff", "Pate de Frango", "Lasagna", "Feijoada")
 
    println(comida)
    */
   // Propriedades
   //
   /*  
   comida.add("Macarrão") // adiciona um valor a lista 
    println(comida)
    comida.add(3,"CusCuz") // adiciona um valor a um index em expecifico na lista   
    println(comida) 
    comida.remove("Pate de Frango") // ele remove um valor em expecifico da lista 
    println(comida) 
    comida.removeAt(2) 
    println(comida) 
    comida.set(2,"Pastel de carne Seca") // substitui um valor pelo seu index
    println(comida) 
    comida.clear() // remover todos os valores da lista
    println(comida)
      
      */
      
